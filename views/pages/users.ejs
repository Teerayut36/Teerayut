<% include ../partials/header %>
 <table class="table">
     <thead>
         <tr>
             <th>ID</th>
             <th>Email</th>
             <th>Password</th>
            <th><a name="" id="" class="btn btn-success" href="/insertuser" role="button">Add New</a></th>
         </tr>
     </thead>
     <tbody>
     <% users.forEach(function(user){ %>
         <tr>
             <td scope="row"><%=user.id %></td>
             <td><%=user.email %></td>
             <td><%=user.password %></td>
             <td><a name="" id="" class="btn btn-dark" href="/users/<%=user.id %>" role="button">Edit</a>
             <a name="" id="" class="btn btn-danger" href="/users/<%=user.id %>" onclick="return confirm('Are you sure you want to delete?')" role="button">Delete</a></td>
         </tr>
         <% }); %>
     </tbody>
 </table>


<% include ../partials/footer %>