<% include ../partials/header %>
<div class="jumbotron">
 <br>
 <h2>Report User</h2>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>E-mail</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody id="myTable">
     <% users.forEach(function(user){ %>
        <tr>
            <td scope="row"><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
        </tr>
      <% }); %>
    </tbody>
</table>
</div>
  <% include ../partials/footer %>